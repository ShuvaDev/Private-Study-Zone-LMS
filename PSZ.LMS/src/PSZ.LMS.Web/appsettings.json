{
    "ConnectionStrings": {
        "DefaultConnection": "Server=192.168.0.102\\SQLEXPRESS,5555;Database=PSZ_LMS;User Id=developer;Password=123456;Trust Server Certificate=True"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.MSSqlServer" ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/web-log-.log",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "MSSqlServer",
                "Args": {
                    "connectionString": "Server=192.168.0.101\\SQLEXPRESS,5555;Database=PSZ_LMS;User Id=developer;Password=123456;Trust Server Certificate=True",

                    "sinkOptionsSection": {
                        "tableName": "Logs",
                        "autoCreateSqlTable": true
                    }
                }
            }
        ]
    }

}
